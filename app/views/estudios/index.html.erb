<%- model_class = Estudio -%>
<p id="notice" <%= notice %></p>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
  <% if policy(@estudios).index? %>
    <%= render partial: "admin_index" %>
  <% else %>
    <%= render partial: "normal_user_index" %>
  <% end %>
</div>
